myApps.controller('dataListing', ['$scope','myData','myFilters',
	function($scope, myData, myFilters){
		$scope.dataName = myData;
		$scope.myfiltersData = myFilters;
		$scope.orderByMe = function(x){
			$scope.myOrderBy = x
		}
	}
])

